<changeSet author="amalik"  id="executeCommand-expdp" runAlways="true" runOnChange="true">  
    <executeCommand  executable="../executeCommand/expdp.sh"  
            timeout="10s">  
        <arg value="USERID"/>
        <arg value="DBHOST"/> 
        <arg value="DUMPFILE.dmp"/> 
        <arg value="LOGFILE.log"/> 
        <arg value="DIRECTORY"/> 
    </executeCommand>  
</changeSet>

<changeSet author="amalik"  id="executeCommand-exp" runAlways="true" runOnChange="true">  
    <executeCommand  executable="../executeCommand/exp.sh"  
            timeout="10s">  
        <arg value="USERID"/>
        <arg value="DBHOST"/> 
        <arg value="OWNER"/> 
        <arg value="DUMPFILE.dmp"/> 
        <arg value="LOGFILE.log"/> 
    </executeCommand>  
</changeSet>

